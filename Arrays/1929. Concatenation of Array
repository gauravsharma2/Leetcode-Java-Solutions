class Solution {
    public int[] getConcatenation(int[] nums) {
        int []ans =new int[2*nums.length];
        int p=0;
        for(int i=0;i<2*nums.length;i++)
        {
            ans[i]=nums[p];
            p++;
            if(p==nums.length)
            p=0;
        }
        return ans;
    }
}
